
<div class="container">

<div class="row">
  <div class="col-xs-4 col-xs-offset-1">
    <h1>Create New Proposal</h1>
  </div>
</div>

<div class="row">
  <div class="col-xs-11 col-xs-offset-1">
    <ol class="breadcrumb decimal">
        <li class="active">1. Funding Opportunity</li>
        <li>2. Unit(s) of Consideration</li>
        <li>3. Submission Type &amp; Funding</li>
        <li>4. Title &amp; Collaborative Info</li>
    </ol>
  </div>
</div>

<div class="row">

    {{#if selectFundingOp}}
      {{wizard/select-funding-op wizard=model.wizard fundingOps=model.fundingOps directorates=model.directorates next=(action 'toDivisionProgram')}}
    {{/if}}

    {{#if selectDivisionProgram}}
      {{wizard/select-division-program wizard=model.wizard divisions=model.divisions previous=(action 'toFundingOp') next=(action 'toTypeFunding')}}
    {{/if}}

    {{#if selectTypeFunding}}
      {{wizard/select-type-funding wizard=model.wizard previous=(action 'toDivisionProgram')}}
    {{/if}}

</div>

{{outlet}}

</div>
